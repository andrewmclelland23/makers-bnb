<div id="log-in-form" class="card col-6 ml-md-auto mr-md-auto form">
    <div class="card-body">
        <h2>Log in</h2>
        <form action="/sessions" method="post">
        <div class="form-group">
            <label for="email">Email address</label>
            <input type="email" class="form-inline" id="email" name="email" placeholder="Enter email" name="email">
        </div>
        <div class="form-group">
            <label for="password">Password</label>
            <input type="password" class="form-inline" id="password" name="password" placeholder="Password">
        </div>
        <button type="submit" class="btn btn-primary">Log in</button>
        </form>
    </div>
</div>
